{
    "name": "spotify-organizer",
    "repository": "https://github.com/hyldmo/spotify-organizer",
    "version": "0.3.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.development.config.js --progress --colors",
        "production": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js --progress --colors",
        "test": "jest",
        "lint": "tslint {__tests__,src}/**/*.{ts,tsx} --type-check -p ./",
        "lint:fix": "npm run lint -- --fix"
    },
    "pre-commit": [
        "lint:fix"
    ],
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
            "\\.(css|less|pcss)$": "<rootDir>/__mocks__/styleMock.ts"
        }
    },
    "keywords": [
        "Typescript",
        "React",
        "Redux",
        "Spotify"
    ],
    "license": "MIT",
    "dependencies": {
        "@types/lodash": "^4.14.74",
        "cross-env": "^5.1.4",
        "history": "^4.6.3",
        "lodash": "^4.17.4",
        "normalize.css": "^8.0.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^4.0.0",
        "redux-form": "^7.0.0",
        "redux-saga": "^0.16.0"
    },
    "devDependencies": {
        "@types/history": "^4.6.0",
        "@types/jest": "^22.2.3",
        "@types/node": "^7.0.31",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-hot-loader": "^4.1.0",
        "@types/react-redux": "*",
        "@types/react-router": "^4.0.14",
        "@types/react-router-dom": "^4.0.6",
        "@types/react-router-redux": "^5.0.3",
        "@types/react-test-renderer": "^16.0.1",
        "@types/redux-actions": "^2.2.4",
        "@types/redux-form": "^7.2.4",
        "@types/spotify-api": "0.0.0",
        "@types/webpack": "^4.1.4",
        "@types/webpack-env": "^1.12.1",
        "awesome-typescript-loader": "^5.0.0",
        "css-loader": "^0.28.11",
        "extract-loader": "^2.0.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^22.4.3",
        "postcss": "^6.0.6",
        "postcss-cssnext": "^3.0.0",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.0.6",
        "pre-commit": "^1.2.2",
        "react-hot-loader": "^4.1.2",
        "react-redux-typescript": "^2.3.0",
        "react-test-renderer": "^16.0.0",
        "redbox-react": "^1.4.3",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.21.0",
        "ts-jest": "^22.4.4",
        "tslint": "^5.6.0",
        "tslint-eslint-rules": "^5.1.0",
        "typescript": "~2.8.3",
        "webpack": "^4.6.0",
        "webpack-dev-server": "3.1.4"
    }
}
