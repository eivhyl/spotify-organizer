{
    "name": "spotify-organizer",
    "repository": "https://github.com/hyldmo/spotify-organizer",
    "version": "0.4.5",
    "scripts": {
        "dev": "npm run compileConf:dev && webpack-dev-server --config webpack.development.config.js --progress --colors --hot",
        "prod": "npm run compileConf:prod && webpack --config webpack.production.config.js --progress --colors",
        "test": "jest",
        "test:cover": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "lint": "tslint {__tests__,src}/**/*.{ts,tsx} webpack.*.ts",
        "stylelint": "stylelint src/**/*.scss --syntax scss",
        "compileConf:dev": "tsc webpack.development.config.ts --lib es6,dom",
        "compileConf:prod": "tsc webpack.production.config.ts --lib es6,dom",
        "stats": "npm run compileConf:prod && webpack --config webpack.production.config.js --profile --json > compilation-stats.json"
    },
    "pre-commit": [
        "lint",
        "stylelint"
    ],
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
            "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.ts"
        }
    },
    "keywords": [
        "Typescript",
        "React",
        "Redux",
        "Spotify"
    ],
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free-solid": "^5.0.13",
        "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
        "@fortawesome/react-fontawesome": "^0.1.0-10",
        "classnames": "^2.2.5",
        "cross-env": "^5.2.0",
        "history": "^4.6.3",
        "lodash": "^4.17.4",
        "material-ui": "^0.20.0",
        "memoizee": "^0.4.11",
        "normalize.css": "^5.0.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.3",
        "@types/history": "^4.6.0",
        "@types/jest": "^23.0.0",
        "@types/lodash": "^4.14.74",
        "@types/material-ui": "^0.21.3",
        "@types/memoizee": "^0.4.2",
        "@types/node": "^10.1.3",
        "@types/react": "16.1.0",
        "@types/react-dom": "16.0.6",
        "@types/react-redux": "^6.0.1",
        "@types/react-router": "^4.0.18",
        "@types/react-router-dom": "^4.2.2",
        "@types/react-router-redux": "^5.0.10",
        "@types/react-test-renderer": "^16.0.0",
        "@types/spotify-api": "0.0.0",
        "@types/webpack": "^4.1.2",
        "@types/webpack-env": "^1.13.5",
        "coveralls": "^3.0.0",
        "css-loader": "^1.0.0",
        "extract-loader": "^0.1.0",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^22.0.4",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.9.0",
        "postcss": "^6.0.6",
        "postcss-loader": "^2.1.4",
        "pre-commit": "^1.2.2",
        "react-hot-loader": "^4.1.2",
        "react-test-renderer": "^16.2.0",
        "redbox-react": "^1.5.0",
        "sass-loader": "^7.0.1",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.19.0",
        "stylelint": "^9.1.3",
        "stylelint-config-recommended-scss": "^3.1.0",
        "stylelint-scss": "^2.5.0",
        "ts-jest": "^23.0.0",
        "ts-loader": "^4.2.0",
        "tslint": "^5.8.0",
        "tslint-eslint-rules": "^4.1.1",
        "typescript": "^2.8.3",
        "webpack": "^4.6.0",
        "webpack-cli": "^3.0.0",
        "webpack-dev-server": "^3.1.3"
    }
}
